DCYN='[0;36m'
DWHI='[0;37m'
RES='[0m'
echo "${GRN} ############################### ${WHI}"
echo "${RED} # H4E MiX-Scanner # H4E Power # ${WHI}"
echo "${GRN} ############################### ${WHI}"
echo "${RED} # Hackers4Ever # Cyber-Mafia  # ${WHI}"
echo "${GRN} ############################### ${WHI}"
